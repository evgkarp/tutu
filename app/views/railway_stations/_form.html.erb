<%= form_with(model: @railway_station, local: true) do |f| %>
<%= render 'common/errors', resource: @railway_station %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, id: :railway_station_title %>
  </div>

  <div class="actions">
    <%= f.submit "#{ @railway_station.new_record? ? 'Создать' : 'Изменить' }" %>
  </div>
<% end %>
